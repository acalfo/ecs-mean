        </div>
        <div class="col-md-10 col-sm-10 col-lg-10 col-xs-12" ng-controller="navController">
          <ul class="nav nav-pills pull-right ecs-nav">
            <li ng-repeat="item in navItems" ng-class="navClass('{{item.path}}')">
