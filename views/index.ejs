            <ul class="nav navbar-nav nav-pills pull-right" style="height:20px;padding-top:0px;">
              <li ng-show="isLoggedIn()" style="height:25px;padding-top:0px;"><a style="padding-top:0px;">{{ currentUser() }}</a></li>
              <li ng-show="isLoggedIn()" style="height:25px;padding-top:0px;"><a href="/#/register" style="padding-top:0px;" ng-click="logOut()">Log Out</a></li>
